{
    "pipeline": {
        "build": {
            "dependsOn": ["^build"],
            "outputs": ["dist/**", ".next/**", "build/**"]
        },
        "dev": {
            "cache": false,
            "persistent": true
        },
        "start": {
            "dependsOn": ["build"]
        },
        "lint": {
            "outputs": []
        },
        "clean": {
            "cache": false
        }
    }
}
